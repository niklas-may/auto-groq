{
  "name": "autogroq",
  "version": "0.0.2",
  "description": "Automatically create groq queries from your sanity.io CMS schemas.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.cjs"
    }
  },
  "main": "./dist/app.cjs",
  "types": "./dist/app.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "unbuild",
    "lint": "eslint --cache --fix . && prettier -c src",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags"
  },
  "packageManager": "pnpm@9.5.0",
  "author": "https://github.com/niklas-may",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.11",
    "@vitest/coverage-v8": "^2.0.3",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "eslint-config-unjs": "^0.3.2",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.3"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "glob": "^11.0.0",
    "groqfmt-nodejs": "^1.0.0",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3"
  }
}
